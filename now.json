{
  "version": 2,
  "name": "now-prisma",
  "builds": [
    {
      "src": "src/index.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "src/index.js"
    }
  ],
  "env": {
    "PRISMA_ENDPOINT": "@prismaendpoint",
    "PRISMA_PASSWORD": "@prismapassword",
    "SECRET_PASSWORD": "@secretpassword"
  }
}