{
  "version": 2,
  "name": "now-prisma",
  "builds": [
    {
      "src": "src/index.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "src/index.js"
    }
  ],
  "env": {
    "PRISMA_ENDPOINT": "@prisma-endpoint",
    "PRISMA_PASSWORD": "@prisma-password"
  }
}